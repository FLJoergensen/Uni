%%% A2 %%%
filter_list(X,List):-
	Out=[],
	filter_list(X,List,Out).
filter_list(X,[],Out):-
	Out.
filter_list(X,[H|R],Out):-
	not(X is H),
	filter_list(X,R,atomic_list_conct(Out,H)).
filter_list(X,[H|R],Out):-
	X is H,
	filter_list(X,R,Out).

t(X,[]):-
	not(true).
t(X,[H|R]):-
	t(X,R),
	t(X,H).
t(X,Y):-
	X=Y.


